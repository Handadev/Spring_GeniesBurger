<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="AddExtraMapper">
  <!-- price는 나중에 추가하면 넣어야함 -->
  	<insert id="reg" parameterType="aevo">
  		insert into tbl_add_extra (aeno, mno, pno, sname)
  		values (seq_add_extra_aeno.nextval, #{mno }, #{pno }, #{sname })
  	</insert>
  	
  	<delete id="del" parameterType="int">
  		delete from tbl_add_extra where mno = #{mno}
  	</delete>
  </mapper>